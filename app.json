{
  "expo": {
    "name": "ticket_unique",
    "slug": "ticket_unique",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "ticketuniquevraiment",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ticketunique.com",
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"]
      },
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground-ut-android.png",
        "backgroundImage": "./assets/background-ut-android.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ticketunique.mobile",
      "versionCode": 1
    },
    "androidStatusBar": {
      "backgroundColor": "#030303",
      "translucent": false,
      "barStyle": "light-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ]
  }
}
